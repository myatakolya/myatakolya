#include <iostream>
#include "cmath"
using namespace std;

double action(double first_num, double second_num){
    double i;
    i = (1.0 / ((first_num) * pow(3, second_num)));
    return i;
}


int main() {
    double answer = sqrt(12.0) * (1 - action(3, 1) + action(5, 2)
            - action(7, 3) + action(9, 4) - action(11, 5));
    cout << answer;
}
