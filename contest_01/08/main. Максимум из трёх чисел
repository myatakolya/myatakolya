package main

import "fmt"

func main() {
	var a float64
	var b float64
	var c float64
	fmt.Scan(&a)
	fmt.Scan(&b)
	fmt.Scan(&c)
	if a > b && a > c {
		fmt.Println(a)
	} else if c > b && c > a {
		fmt.Println(c)
	} else if b > c && b > a {
		fmt.Println(b)
	} else {
		fmt.Println(a)
	}

}
