#include <iostream>
using namespace std;

int action(int n){
    int num = 1;
    int floor = 3;
    int x = 1, f = 1;
    while(num<=n){
        for (int i=0; i<f && num<=n; i++)
            cout << num++ << ' ';
        cout << endl;
        f += x;
        if (f < 1){
            x = 1;
            f = 2;
            floor++;
        }else if (f>=floor){
            x = -1;
            f-=2;
        }
    }
}

int main(){
    int n;
    cin >> n;
    action(n);
}